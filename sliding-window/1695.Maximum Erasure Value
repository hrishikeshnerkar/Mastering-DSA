class Solution {
    public int maximumUniqueSubarray(int[] nums) {
        int left=0, sum=0, maxSum=0;
        HashSet<Integer> hs = new HashSet<>();
        for(int right=0; right<nums.length; right++){
            while(hs.contains(nums[right])){
                hs.remove(nums[left]);
                sum -= nums[left];
                left++;
            }
            sum += nums[right];
            hs.add(nums[right]);
            maxSum = Math.max(maxSum, sum);
        }
        return maxSum;
    }
}